<p id="notice"><%= notice %></p>





<div class="link row clearfix">
  <h1 style="text-align: left;"><%= @segment.travelsegments %></h1>
</div>

<% if @segment.seller==current_seller %>

	<div class="btn-group">
		<%= link_to 'Edit', edit_segment_path(@segment), class: "btn btn-default" %>
		<%= link_to 'Back', segments_path, class: "btn btn-default" %>
		<%= link_to 'Destroy', @segment, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-default" %>
	</div>
<% end %>