<p id="notice"><%= notice %></p>
<% if current_user==@my_query.user %>
<table style="border: 1px solid black;"">
  <thead>
    <tr>
      <th  style="padding: 30px;">Query Number</th>
      <th  style="padding: 30px;">Destination</th>
      <th  style="padding: 30px;">No.of Pax</th>
      <th  style="padding: 30px;">travel date</th>
      <th  style="padding: 30px;">Duration</th>
      <th  style="padding: 30px;">Tour Requirements</th>
      <th  style="padding: 30px;">Quotes Received</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td style="padding: 30px;"><%= @my_query.query_number %></td>
        <td style="padding: 30px;"><%= @my_query.destination %></td>
        <td style="padding: 30px;"><%= @my_query.pax %></td>
        <td style="padding: 30px;"><%= @my_query.travel_date %></td>
        <td style="padding: 30px;"><%= @my_query.duration %></td>
        <td style="padding: 30px;"><%= @my_query.tour_requirements %></td>
        <td style="padding: 30px;"><%= @my_query. quotes_received%></td>
     
        <td style="padding: 30px;"><%= link_to 'Edit', edit_my_query_path(@my_query) %></td>
        <td style="padding: 30px">	<%= link_to 'Back', my_queries_path %></td>
      
       
      </tr>
    
  </tbody>
</table>
<% else %>
	<%= link_to 'My Query', new_my_query_path %>
<% end %>



