<p id="notice"><%= notice %></p>

<h1>Commentreplies</h1>

<table>
  <thead>
    <tr>
      <th>Body</th>
      <th>Comment</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commentreplies.each do |commentreply| %>
      <tr>
        <td><%= commentreply.body %></td>
        <td><%= commentreply.comment_id %></td>
        <td><%= commentreply.user %></td>
        <td><%= link_to 'Show', commentreply %></td>
        <td><%= link_to 'Edit', edit_commentreply_path(commentreply) %></td>
        <td><%= link_to 'Destroy', commentreply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commentreply', new_commentreply_path %>
